{
  "name": "loan-app-template",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:theme:default": "node-sass -c ./src/assets/theme/default/theme.scss -o ./src/assets/theme/default/",
    "build:theme:pink": "node-sass -c ./src/assets/theme/pink/theme.scss -o ./src/assets/theme/pink/",
    "build:theme:pink-blue-gray": "node-sass -c ./src/assets/theme/pink-blue-gray/theme.scss -o ./src/assets/theme/pink-blue-gray/",
    "build:theme:green-blue-gray": "node-sass -c ./src/assets/theme/green-blue-gray/theme.scss -o ./src/assets/theme/green-blue-gray/",
    "build:themes": "npm run build:theme:default && npm run build:theme:pink && npm run build:theme:pink-blue-gray && npm run build:theme:green-blue-gray ",
    "gen:tmpl:meta": "node utilities/generate-template-meta.js",
    "gen:tmpl": "npm run build:themes && ng build --prod && npm run gen:tmpl:meta"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.0.0",
    "@angular/cdk": "^10.2.7",
    "@angular/common": "~10.2.0",
    "@angular/compiler": "~10.2.0",
    "@angular/core": "~10.2.0",
    "@angular/forms": "~10.2.0",
    "@angular/material": "^10.2.7",
    "@angular/platform-browser": "~10.2.0",
    "@angular/platform-browser-dynamic": "~10.2.0",
    "@angular/router": "~10.2.0",
    "@types/jquery": "^3.5.4",
    "ace-lib-theme": "^1.0.6",
    "hammerjs": "^2.0.8",
    "jquery": "^3.5.1",
    "jquery-steps": "^1.1.0",
    "node-sass": "^5.0.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.0",
    "@angular/cli": "~10.2.0",
    "@angular/compiler-cli": "~10.2.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jsonfile": "^6.1.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}